FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "system";
    object          sampleDict;
}

type            sets;

libs            ( "libsampling.so" );

interpolationScheme cellPoint;

setFormat       raw;

sets            ( data { type lineUniform ; libs ( "libsampling.so" ) ; axis distance ; start ( 0.5 0 0 ) ; end ( 1 0 0 ) ; nPoints 1 ; } );

fields          ( PI_62 );


